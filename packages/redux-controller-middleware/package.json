    "cheap-di": "^4.0.0-rc-32",
    "cheap-di-react": "^4.1.0-rc-32",
    "eslint": "^8.55.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "redux": "^5.0.0",
