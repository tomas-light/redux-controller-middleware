{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "coverageDirectory": "<rootDir>/test-coverage",
  "coverageReporters": ["json", "html"],
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{ts,tsx}"
  ],
  "coveragePathIgnorePatterns": ["index.ts"],
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest"
    ]
  }
}
